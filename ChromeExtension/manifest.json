{
    "manifest_version": 3,
    "name": "Tiktok in full-screen Extension",
    "version": "1.0",
    "description": "Tiktok in full-screen Extension",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [ "*://tiktok.com/*", "*://www.tiktok.com/*"],
            "css": [
                "content.css"
            ]
        }
    ],
    "action": {
    }
}